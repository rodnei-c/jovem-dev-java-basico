package aula5.stream;

import java.util.ArrayList;
import java.util.List;

public class Exercicio5 {
	public static void main(String[] args) {
		
		List<String> nome = new ArrayList<>();
		
		nome.add("douglas");
		nome.add("valdemiro");
		nome.add("mirosvaldo");
		nome.add("ced");
		nome.add("dmoc");
		nome.add("roberto");
		nome.add("adalberto");
		nome.add("bertones");
		nome.add("gabigol");
		nome.add("solaire");
		
		nome.stream().filter(elemento -> elemento.endsWith("berto"))
		
	}
}
